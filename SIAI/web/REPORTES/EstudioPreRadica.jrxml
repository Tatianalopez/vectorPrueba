	<queryString>
		<![CDATA[select PR.Cod_Preradica, ifnull(concat(cli.Numero_DocCliente,' - ',cli.nombre_cliente),'Ninguno') as nombres_cliente,
                cli.Telefono_Cliente,cli.Mail_Cliente,cli.Direccion_Cliente,Solicitante_PreRadica,
                if(P.Resultado_Parametro='Otro', PR.DetalleEnvio_PreRadica,P.Resultado_Parametro) as EnviarA,
                (select count(cod_factura)  from factura
                left join persona_factura on fk_cod_factura = cod_factura
                left join cliente c1 on fk_cod_cliente = c1.cod_cliente
                where c1.cod_cliente = $P{cod_cliente} and estado_factura not in ('N','P','R','SP')) as Total_Facturas
                from Cliente cli 
                left join Cliente_Preradica CPR on CPR.FK_Cod_Cliente = cli.Cod_Cliente
                left join Pre_Radicacion PR on PR.Cod_PreRadica = CPR.FK_Cod_PreRadica
                left join Parametro P on P.Cod_Parametro = PR.EnvioInformacion_PreRadica
                where PR.Cod_PreRadica = $P{cod_pre_radica} and cli.Cod_Cliente = $P{cod_cliente};]]>
	</queryString>
