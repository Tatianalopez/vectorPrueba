	<queryString>
		<![CDATA[Select concat(P.Nombre_Perito,' ',P.Apellido_Perito) NomPer,concat(P.Telefono_Perito,' - ',P.Celular_Perito) NumTel,
A.Cod_Avaluo CodAvaluo,S.Direccion as DireccAva,
concat(D.Nombre_Departamento,' - ',C.Nombre_Ciudad) Ubicacion ,concat(D1.Nombre_Departamento,' - ',C1.Nombre_Ciudad) UbiPer,
(Select concat(CL.Nombre_Cliente) NomCli from Cliente CL left join 
ValpacCliente VPCL on VPCL.FK_Cod_Cliente=CL.Cod_Cliente
left join Avaluo A1 on A1.Cod_Avaluo=VPCL.FK_Cod_Avaluo
left join Seguimiento S1 on S1.FK_Cod_Avaluo=A1.Cod_Avaluo
where A1.Cod_Avaluo=A.Cod_Avaluo and S1.Numero_Bien_Seguimiento=S.Numero_Bien_Seguimiento
order by VPCL.Cod_ValPacCliente asc limit 1
) as Propietario,
concat(CTO.Nombre_Contacto,' ',CTO.Celular_Contacto,' - ',CTO.Telefono_Contacto) NumContac,
ifnull((Select concat(E.Nombre_Entidad,' ',SC.Nombre_Oficina) from Asesor ASE
left join Valpac_Asesor VPCA on VPCA.FK_Cod_Asesor=ASE.Cod_Asesor
left join Avaluo A2 on A2.Cod_Avaluo=VPCA.FK_Cod_Avaluo
left join Seguimiento S2 on S2.FK_Cod_Avaluo=A2.Cod_Avaluo
left join Sucursal SC on SC.Cod_Sucursal=ASE.FK_Cod_Sucursal
left join Entidad E on E.Cod_Entidad=SC.FK_Cod_Entidad
where A2.Cod_Avaluo=A.Cod_Avaluo and S2.Numero_Bien_Seguimiento=S.Numero_Bien_Seguimiento
order by VPCA.Cod_ValPacAsesor asc limit 1),'--') Entid,
ifnull(Cit.Fecha_Cita,'No ha sido agendada') Fecha_Cita
from Perito P 
left join Seguimiento_Perito SP on SP.FK_Cod_Perito=P.Cod_Perito
left join Seguimiento S on S.Cod_Seguimiento=SP.FK_Cod_Seguimiento
left join Ciudad C on C.Cod_Ciudad=S.FK_Cod_Ciudad 
left join Departamento D on D.Cod_Departamento=C.FK_Cod_Departamento
left join Ciudad C1 on C1.Cod_Ciudad=P.FK_Cod_Ciudad 
left join Departamento D1 on D1.Cod_Departamento=C1.FK_Cod_Departamento
left join Avaluo A on A.Cod_Avaluo=S.FK_Cod_Avaluo
left join Contacto CTO on CTO.FK_Cod_Avaluo=A.Cod_Avaluo
left join Cita Cit on Cit.FK_Cod_Seguimiento = S.Cod_Seguimiento
where A.Cod_Avaluo=$P{CodAval} and S.Numero_Bien_Seguimiento=$P{NBien} and  P.cod_perito=$P{cod_perito} and A.Estado_Avaluo='P';]]>
	</queryString>
