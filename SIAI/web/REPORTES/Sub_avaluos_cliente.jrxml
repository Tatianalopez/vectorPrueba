	<subDataset name="AvaluosCliente" uuid="7825121b-c32d-4633-9eb1-76ba06d4e67b">
		<parameter name="cod_cliente_sub_tabla" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select av.*,ifnull(s.ValorAvaluo,'No registra') as ValorAvaluo,
concat(depto_aval.Nombre_Departamento, ' - ' ,ciu_aval.Nombre_Ciudad) as ubicacion_aval,
s.Direccion as direccionAva from Avaluo av 
inner join ValpacCliente vpc on vpc.FK_Cod_Avaluo = av.Cod_Avaluo
inner join Cliente cli on cli.Cod_Cliente = vpc.FK_Cod_Cliente
inner join Seguimiento s on s.FK_Cod_Avaluo = av.Cod_Avaluo
inner join Ciudad ciu_aval on ciu_aval.Cod_Ciudad = s.FK_Cod_Ciudad
inner join Departamento depto_aval on depto_aval.Cod_Departamento = ciu_aval.FK_Cod_Departamento 
where vpc.FK_Cod_Cliente = $P{cod_cliente_sub_tabla} order by av.Fecha_Radicado desc limit 3;]]>
		</queryString>
		<field name="Cod_Avaluo" class="java.lang.Integer"/>
		<field name="Fecha_Solicitud" class="java.sql.Timestamp"/>
		<field name="Fecha_Radicado" class="java.sql.Timestamp"/>
		<field name="Estado_Avaluo" class="java.lang.String"/>
		<field name="FK_Cod_Ciudad" class="java.lang.Integer"/>
		<field name="FK_Cod_TipAvaluo" class="java.lang.Integer"/>
		<field name="FK_Cod_TipProEnt" class="java.lang.Integer"/>
		<field name="FK_Cod_Empleados" class="java.lang.Integer"/>
		<field name="ValorAvaluo" class="java.lang.String">
			<fieldDescription><![CDATA[Valor comercial del avaluo con el que sale de la organización una vez sea facturad]]></fieldDescription>
		</field>
		<field name="ubicacion_aval" class="java.lang.String"/>
		<field name="direccionAva" class="java.lang.String"/>
	</subDataset>
	<parameter name="cod_cliente_sub" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select count(*) as Cantidad_aval_cliente
from Avaluo av 
left join ValpacCliente vpc on vpc.FK_Cod_Avaluo = av.Cod_Avaluo
left join Cliente cli on cli.Cod_Cliente = vpc.FK_Cod_Cliente
where vpc.FK_Cod_Cliente = $P{cod_cliente_sub} order by av.Fecha_Radicado desc limit 3;]]>
	</queryString>
	<field name="Cantidad_aval_cliente" class="java.lang.Long"/>
