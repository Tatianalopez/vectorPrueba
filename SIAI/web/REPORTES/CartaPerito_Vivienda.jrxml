	<subDataset name="CartaPerVivie" uuid="f4d5813c-fd51-4621-9717-9b06692d89ce">
		<parameter name="cod_avaluo_tabla" class="java.lang.String"/>
		<parameter name="cod_perito_tabla" class="java.lang.String"/>
		<parameter name="cod_bien_seguimiento_tabla" class="java.lang.String"/>
		<queryString>
			<![CDATA[Select concat(A.Cod_Avaluo,' - ',S.Numero_Bien_Seguimiento) CodAvaluo,S.Direccion direccionAva,concat(DA.Nombre_Departamento, ' ',CA.Nombre_Ciudad) ubicacion_aval,
CL.Nombre_Cliente nombre_cliente,CO.Nombre_Contacto,CO.Telefono_Contacto,CO.Celular_Contacto,
concat(N.Nombre_Entidad,' - ',SU.Nombre_Oficina) nombre_entidad,concat(P.Nombre_Perito, ' ',P.Apellido_Perito) nombre_perito,P.Celular_Perito,
concat(DP.Nombre_Departamento, ' ',CP.Nombre_Ciudad) ubicacion_peri from Avaluo A 
left join Seguimiento S on S.FK_Cod_Avaluo=A.Cod_Avaluo
left join ValpacCliente VC on VC.FK_Cod_Avaluo=A.Cod_Avaluo
left join Cliente CL on CL.Cod_Cliente= VC.FK_Cod_Cliente
left join Ciudad CA on CA.Cod_Ciudad= S.FK_Cod_Ciudad
left join Departamento DA on DA.Cod_Departamento=CA.FK_Cod_Departamento
left join Contacto CO on CO.FK_Cod_Avaluo =A.Cod_Avaluo
left join Valpac_Asesor VA on VA.FK_Cod_Avaluo=A.Cod_Avaluo
left join Asesor AR on AR.Cod_Asesor=VA.FK_Cod_Asesor
left join Sucursal SU on SU.Cod_Sucursal=AR.FK_Cod_Sucursal
left join Entidad N on N.Cod_Entidad=SU.FK_Cod_Entidad
left join Seguimiento_Perito SP on SP.FK_Cod_Seguimiento=S.Cod_Seguimiento
left join Perito P on P.Cod_Perito=SP.FK_Cod_Perito
left join Ciudad CP on CP. Cod_Ciudad=P.FK_Cod_Ciudad
left join Departamento DP on DP.Cod_Departamento=CP.FK_Cod_Departamento
where P.Cod_Perito=$P{cod_perito_tabla} and A.Cod_Avaluo=$P{cod_avaluo_tabla} and S.Numero_Bien_Seguimiento=$P{cod_bien_seguimiento_tabla} GROUP BY P.Cod_Perito]]>
		</queryString>
		<field name="CodAvaluo" class="java.lang.String"/>
		<field name="direccionAva" class="java.lang.String"/>
		<field name="ubicacion_aval" class="java.lang.String"/>
		<field name="nombre_cliente" class="java.lang.String">
			<fieldDescription><![CDATA[Nombre del cliente o de la organizacion]]></fieldDescription>
		</field>
		<field name="Nombre_Contacto" class="java.lang.String">
			<fieldDescription><![CDATA[Nombre de la persona encargada de mostrar el inmueble]]></fieldDescription>
		</field>
		<field name="Telefono_Contacto" class="java.lang.String">
			<fieldDescription><![CDATA[Numero de telefono en el que se le contacta a la persona]]></fieldDescription>
		</field>
		<field name="Celular_Contacto" class="java.lang.String">
			<fieldDescription><![CDATA[Numero de celular en el cual se le hara contacto a la persona]]></fieldDescription>
		</field>
		<field name="nombre_entidad" class="java.lang.String">
			<fieldDescription><![CDATA[Nombre de la entidad]]></fieldDescription>
		</field>
		<field name="nombre_perito" class="java.lang.String">
			<fieldDescription><![CDATA[Nombre del perito]]></fieldDescription>
		</field>
		<field name="Celular_Perito" class="java.lang.String">
			<fieldDescription><![CDATA[Numero de celular del avaluador]]></fieldDescription>
		</field>
		<field name="ubicacion_peri" class="java.lang.String"/>
	</subDataset>
	<parameter name="cod_perito" class="java.lang.String"/>
	<parameter name="cod_avaluo" class="java.lang.String"/>
	<parameter name="cod_bien_seguimiento" class="java.lang.String"/>
	<queryString>
		<![CDATA[Select concat(A.Cod_Avaluo,' - ',S.Numero_Bien_Seguimiento) CodAvaluo,S.Direccion direccionAva,concat(DA.Nombre_Departamento, ' ',CA.Nombre_Ciudad) ubicacion_aval,
CL.Nombre_Cliente nombre_cliente,CO.Nombre_Contacto,CO.Telefono_Contacto,CO.Celular_Contacto,
concat(N.Nombre_Entidad,' - ',SU.Nombre_Oficina) nombre_entidad,concat(P.Nombre_Perito, ' ',P.Apellido_Perito) nombre_perito,P.Celular_Perito,
concat(DP.Nombre_Departamento, ' ',CP.Nombre_Ciudad) ubicacion_peri,
ifnull(Cit.Fecha_Cita,'No ha sido agendada') Fecha_Cita from Avaluo A 
left join Seguimiento S on S.FK_Cod_Avaluo=A.Cod_Avaluo
left join ValpacCliente VC on VC.FK_Cod_Avaluo=A.Cod_Avaluo
left join Cliente CL on CL.Cod_Cliente= VC.FK_Cod_Cliente
left join Ciudad CA on CA.Cod_Ciudad= S.FK_Cod_Ciudad
left join Departamento DA on DA.Cod_Departamento=CA.FK_Cod_Departamento
left join Contacto CO on CO.FK_Cod_Avaluo =A.Cod_Avaluo
left join Valpac_Asesor VA on VA.FK_Cod_Avaluo=A.Cod_Avaluo
left join Asesor AR on AR.Cod_Asesor=VA.FK_Cod_Asesor
left join Sucursal SU on SU.Cod_Sucursal=AR.FK_Cod_Sucursal
left join Entidad N on N.Cod_Entidad=SU.FK_Cod_Entidad
left join Seguimiento_Perito SP on SP.FK_Cod_Seguimiento=S.Cod_Seguimiento
left join Perito P on P.Cod_Perito=SP.FK_Cod_Perito
left join Ciudad CP on CP. Cod_Ciudad=P.FK_Cod_Ciudad
left join Departamento DP on DP.Cod_Departamento=CP.FK_Cod_Departamento
left join Cita Cit on Cit.FK_Cod_Seguimiento = S.Cod_Seguimiento
where P.Cod_Perito=$P{cod_perito} and A.Cod_Avaluo=$P{cod_avaluo} and S.Numero_Bien_Seguimiento=$P{cod_bien_seguimiento} GROUP BY P.Cod_Perito]]>
	</queryString>
